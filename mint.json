{
    "$schema": "https://mintlify.com/schema.json",
    "name": "Crease.cloud Docs",
    "logo": {
      "dark": "/logos/logotype-dark.svg",
      "light": "/logos/logotype.svg",
      "href": "https://crease.cloud"
    },
    "favicon": "/logos/favicon.svg",
    "colors": {
      "primary": "#1D1D1D",
      "light": "#F2F1EF",
      "dark": "#1D1D1D",
      "background": {
        "dark": "#121212",
        "light": "#FFFFFF"
      },
      "anchors": {
        "from": "#FF7F57",
        "to": "#9563FF"
      }
    },
    "topbarLinks": [
      {
        "name": "Support",
        "url": "mailto:support@crease.cloud"
      }
    ],
    "topbarCtaButton": {
      "name": "Dashboard",
      "url": "https://app.crease.cloud"
    },
    "openapi": "https://api.crease.cloud",
    "tabs": [
      {
        "name": "API Reference",
        "url": "api-reference"
      }
    ],
    "anchors": [
      {
        "name": "Help Center",
        "icon": "question",
        "url": "https://crease.cloud/help"
      }
    ],
    "navigation": [
      {
        "group": "Getting Started",
        "pages": ["introduction", "local-development", "self-hosting"]
      },
      {
        "group": "Resources",
        "pages": ["integrations", "sdks", "examples"]
      }
    ],
    "api": {
      "maintainOrder": true
    },
    "feedback": {
      "thumbsRating": true
    },
    "modeToggle": {
      "default": "light",
      "isHidden": true
    },
    "footerSocials": {
      "website": "https://crease.cloud"
    }
  }
